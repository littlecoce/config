general {
	colors = true
}

order += "ethernet enp0s3"
order += "load"
order += "disk /"
order += "tztime local"

ethernet enp0s3 {
	format_up = "enp0s3: %ip"
	format_down = "enp0s3: Down"
}

tztime local {
	format = "%d %m %Y %H:%M:%S"
}

load {
	format = "Load:5min"
}

disk "/" {
	format = "%used/%free"
}
